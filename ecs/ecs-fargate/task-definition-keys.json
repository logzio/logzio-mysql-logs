{
   "requiresCompatibilities":[
      "FARGATE"
   ],
   "containerDefinitions":[
      {
         "logConfiguration":{
            "logDriver":"awslogs",
            "options":{
               "awslogs-group":"/aws/ecs/logzio-mysql-logs",
               "awslogs-region":"<<AWS-REGION>>",
               "awslogs-stream-prefix":"aws/ecs"
            }
         },
         "portMappings":[
            
         ],
         "environment":[
            {
               "name":"LOGZIO_TOKEN",
               "value":"<<LOGZIO-LOGS-SHIPPING-TOKEN>>"
            },
            {
               "name":"RDS_IDENTIFIER",
               "value":"<<RDS-IDENTIFIER>>"
            },
            {
               "name": "AWS_REGION",
               "value": "<<AWS-REGION>>",
            },
            {
               "name":"AWS_ACCESS_KEY",
               "value":"<<AWS-ACCESS-KEY>>"
            },
            {
               "name":"AWS_SECRET_KEY",
               "value":"<<AWS-SECRET-KEY>>"
            },
            {
               "name":"RDS_ERROR_LOG_FILE",
               "value":"<<RDS-ERROR-LOG-FILE-PATH>>"
            },
            {
               "name":"RDS_SLOW_LOG_FILE",
               "value":"<<RDS-SLOW-LOG-FILE-PATH>"
            },
            {
               "name":"RDS_LOG_FILE",
               "value":"<<RDS-LOG-FILE-PATH>>"
            }
         ],
         "mountPoints":[
            {
               "readOnly":false,
               "containerPath":"/var/log/logzio",
               "sourceVolume":"varloglogzio"
            },
            {
               "readOnly":false,
               "containerPath":"/var/log/mysql",
               "sourceVolume":"varlogmysql"
            }
         ],
         "memoryReservation":512,
         "volumesFrom":[
            
         ],
         "image":"docker.io/logzio/mysql-logs:1.0.0",
         "essential":true,
         "readonlyRootFilesystem":false,
         "name":"logzio-mysql-logs"
      }
   ],
   "volumes":[
      {
         "name":"varloglogzio"
      },
      {
         "name":"varlogmysql"
      }
   ],
   "networkMode":"awsvpc",
   "memory":"1024",
   "cpu":"512",
   "executionRoleArn":"<<YOUR-EXECUTION-ROLE-ARN>>",
   "family":"logzio-mysql-logs",
   "taskRoleArn":"",
   "runtimePlatform":{
      "operatingSystemFamily":"LINUX"
   },
   "tags":[
      
   ]
}